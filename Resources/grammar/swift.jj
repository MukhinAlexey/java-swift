PARSER_BEGIN(JavaSwift)

public class JavaSwift
{
    public static void main(String args[]) throws ParseException
    {
        JavaSwift parser = new JavaSwift(System.in);
        while (true)
        {
            parser.TopLevelDeclaration();
        }
    }
}

PARSER_END(JavaSwift)

